{
    "name": "dogma/dogma-dev",
    "type": "library",
    "description": "Development tools and dependencies for Dogma and other projects.",
    "homepage": "http://github.com/paranoiq/dogma-dev",
    "license": ["BSD-3-Clause"],
    "authors": [
        {
            "name": "Vlasta Neubauer",
            "homepage": "http://www.paranoiq.cz"
        }
    ],
    "require": {
        "php-64bit": "^7.1.0",
        "maglnet/composer-require-checker": "~1.1",
        "nette/tester": "~2.3.1",
        "phing/phing": "~3.0.0a3",
        "php-parallel-lint/php-console-highlighter": "~0.3",
        "php-parallel-lint/php-parallel-lint": "~1.0",
        "phpstan/phpstan": "0.12.18",
        "slevomat/coding-standard": "6.2.0",
        "squizlabs/php_codesniffer": "^3.5.4",
        "spell-checker/spell-checker": "~0.3",
        "spell-checker/dictionary-en": "~0.1",
        "tracy/tracy": "~2.4"
    },
    "suggest": {
        "ext-sockets": "For debug-server.php and debug-client.php"
    },
    "autoload": {
        "classmap": ["src"]
    },
    "config": {
        "bin-dir": "bin"
    }
}
